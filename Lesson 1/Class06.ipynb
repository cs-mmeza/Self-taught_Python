    "planetas = [\"venus\", \"tierra\", \"saturno\"]\n",
    "\n",
    "# while loop\n",
    "i = 0\n",
    "while(i < len(planetas)) :\n",
    "    print(planetas[i])\n",
    "    \n",
    "    i += 1 # = i = i + 1\n",
    "\n",
    "# for loop\n",
    "for planeta in planetas:\n",
    "    print(planeta)"
